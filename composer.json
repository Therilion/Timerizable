    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Escobar",
            "email": "pcadicts@gmail.com",
            "homepage": "https://gitlab.com/therilion"
        }
    ],
    "homepage": "https://github.com/therilion/timerizable",
    "keywords": ["Laravel", "Timerizable"],
    "require": {
        "illuminate/database": "~5",
        "illuminate/http": "~5",
        "illuminate/routing": "~5",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Therilion\\Timerizable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Therilion\\Timerizable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Therilion\\Timerizable\\TimerizableServiceProvider"
            ],
            "aliases": {
                "Timerizable": "Therilion\\Timerizable\\Facades\\Timerizable"
            }
        }
    }
}
